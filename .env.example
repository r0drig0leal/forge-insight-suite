# Environment Variables for PropertyForge AI
# Copy this file to .env.local and update the values as needed

# API Configuration (Backend API)
VITE_API_BASE_URL=https://api.propertyforgeai.com
VITE_API_TIMEOUT=5000

# Authentication
VITE_API_BEARER_TOKEN=7f2e1c9a-auctions-2025
VITE_API_KEY=7f2e1c9a-auctions-2025

# Development Server Configuration
# Frontend runs on port 8080, Backend API on port 3000

# Feature Flags
VITE_ENABLE_VOICE_INPUT=true
VITE_ENABLE_ADDRESS_AUTOCOMPLETE=true
VITE_DEBUG_MODE=false

# Application Settings
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# Autocomplete Settings
VITE_AUTOCOMPLETE_MIN_SEARCH_LENGTH=2
VITE_AUTOCOMPLETE_DEBOUNCE_DELAY=300
VITE_AUTOCOMPLETE_MAX_RESULTS=8